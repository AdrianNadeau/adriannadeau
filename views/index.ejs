  <script>
    // Overlay bullet points data
    const overlayBullets = [
      { icon: '<i class="uil uil-trophy" style="color:#2979FF;"></i>', text: 'Performance-First Model: We get paid when you\'re happy with the results: Win/Win.' },
      { icon: '<i class="uil uil-dollar-sign" style="color:#FFD600;"></i>', text: 'Low or No Upfront Costs: Keep your cash flow free for other priorities.' },
      { icon: '<i class="uil uil-gift" style="color:#00C853;"></i>', text: 'Funding Opportunities: Potential coverage for up to 75% of costs via Digital Boost 3.0.' },
      { icon: '<i class="uil uil-award" style="color:#6f42c1;"></i>', text: '20+ Years of Experience in IT: Delivering solutions across multiple industries and challenges.' },
      { icon: '<i class="uil uil-rocket" style="color:#FF9100;"></i>', text: 'End-to-End Execution: From planning to delivery, we\'ve got you covered.' }
    ];
    let overlayIndex = 0;
    let overlayTimeout;
    function showOverlayBullet(idx, animate = true) {
      const overlayDiv = document.getElementById('overlay-bullet');
      const iconSpan = document.getElementById('overlay-bullet-icon');
      const textSpan = document.getElementById('overlay-bullet-text');
      if (!iconSpan || !textSpan || !overlayDiv) return;
      iconSpan.innerHTML = overlayBullets[idx].icon;
      textSpan.textContent = overlayBullets[idx].text;
      if (animate) {
        overlayDiv.style.opacity = 0;
        overlayDiv.style.transform = 'translate(-30%, -50%)';
        setTimeout(() => {
          overlayDiv.style.transition = 'opacity 0.5s, transform 0.5s';
          overlayDiv.style.opacity = 1;
          overlayDiv.style.transform = 'translate(-50%, -50%)';
        }, 50);
        setTimeout(() => {
          overlayDiv.style.transition = '';
        }, 600);
      }
    }
    function nextOverlayBullet() {
      overlayIndex = (overlayIndex + 1) % overlayBullets.length;
      showOverlayBullet(overlayIndex);
      resetOverlayTimeout();
    }
    function prevOverlayBullet() {
      overlayIndex = (overlayIndex - 1 + overlayBullets.length) % overlayBullets.length;
      showOverlayBullet(overlayIndex);
      resetOverlayTimeout();
    }
    function resetOverlayTimeout() {
      clearTimeout(overlayTimeout);
      overlayTimeout = setTimeout(nextOverlayBullet, 4000);
    }
    document.addEventListener('DOMContentLoaded', function () {
      showOverlayBullet(overlayIndex);
      resetOverlayTimeout();
      document.getElementById('overlay-prev').onclick = prevOverlayBullet;
      document.getElementById('overlay-next').onclick = nextOverlayBullet;
    });
  </script>

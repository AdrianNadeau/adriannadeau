      whatWeDo.scrollIntoView({ behavior: 'smooth', block: 'center' });
      setTimeout(function() {
        whatWeDo.style.transition = 'opacity 0.6s, transform 0.6s';
        whatWeDo.style.opacity = 1;
        whatWeDo.style.transform = 'translateY(0)';
      }, 300);
      setTimeout(function() {
        whatWeDo.style.transition = '';
      }, 1000);
    });
  }
});
